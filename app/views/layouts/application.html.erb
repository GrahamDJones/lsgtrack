<!DOCTYPE html>
<html>
  <head>
    <%= render "layouts/window_title" %>
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= render "layouts/dynamic_styles" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render "layouts/notices" %>
    <%= render "layouts/page_header" %>
    <%= yield %>
    <%= render "layouts/page_footer" %>
  </body>
</html>
